																								Formula="IIF(@Data.agg_cYTD~==0,0,@Data.agg_cGYTD~/@Data.agg_cYTD~)"
																								ID="calc_cGYTDpct"
																							/>
																							<CalculatedColumn
																								ErrorResult="0"
																								Formula="IIF(@Data.agg_pMTD~==0,0,@Data.agg_pGMTD~/@Data.agg_pMTD~)"
