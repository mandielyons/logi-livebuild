																						<DataLayer
																							ConnectionID="connProdGP"
																							ID="sqlSales"
																							Source="SELECT COALESCE(cm.MNACSGMT,cy.MNACSGMT,pm.MNACSGMT,py.MNACSGMT) MNACSGMT, COALESCE(cm.ACTDESCR, cy.ACTDESCR, pm.ACTDESCR, py.ACTDESCR) ACTDESCR, SUM(ISNULL(cm.AMT,0))*-1 cMTD, SUM(ISNULL(cm.GM,0))*-1 cGMTD, CASE WHEN SUM(ISNULL(cm.AMT,0))=0 THEN 0 ELSE SUM(ISNULL(cm.GM,0))/SUM(ISNULL(cm.AMT,0)) END cGMTDpct, SUM(ISNULL(cy.AMT,0))*-1 cYTD, SUM(ISNULL(cy.GM,0))*-1 cGYTD, CASE WHEN SUM(ISNULL(cy.AMT,0))=0 THEN 0 ELSE SUM(ISNULL(cy.GM,0))/SUM(ISNULL(cy.AMT,0)) END cGYTDpct, SUM(ISNULL(pm.AMT,0))*-1 pMTD, SUM(ISNULL(pm.GM,0))*-1 pGMTD, CASE WHEN SUM(ISNULL(pm.AMT,0))=0 THEN 0 ELSE SUM(ISNULL(pm.GM,0))/SUM(ISNULL(pm.AMT,0)) END pGMTDpct, SUM(ISNULL(py.AMT,0))*-1 pYTD, SUM(ISNULL(py.GM,0))*-1 pGYTD, CASE WHEN SUM(ISNULL(py.AMT,0))=0 THEN 0 ELSE SUM(ISNULL(py.GM,0))/SUM(ISNULL(py.AMT,0)) END pGYTDpct
FROM 
      ( SELECT s.JSTR, COALESCE(s.MNACSGMT,p.MNACSGMT) MNACSGMT, COALESCE(s.ACTDESCR,p.ACTDESCR) ACTDESCR, ISNULL(s.AMT,0) AMT, ISNULL(s.AMT,0)+ISNULL(p.AMT,0) as GM, CASE ISNULL(s.AMT,0) WHEN 0 THEN 0 ELSE (ISNULL(s.AMT,0)+ISNULL(p.AMT,0))/s.AMT END GMPCT FROM 
	    ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, CASE MNACSGMT WHEN &apos;4500&apos; THEN 0 ELSE SUM(PERDBLNC) END AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~  ) AND PERIODID=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4300&apos; AND &apos;4500&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) s FULL OUTER JOIN 
        ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~  ) AND PERIODID =@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4500&apos; AND &apos;5599&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) p ON s.JSTR=p.JSTR
       ) cm 
	   FULL OUTER JOIN 
      ( SELECT s.JSTR, COALESCE(s.MNACSGMT,p.MNACSGMT) MNACSGMT, COALESCE(s.ACTDESCR,p.ACTDESCR) ACTDESCR, ISNULL(s.AMT,0) AMT, ISNULL(s.AMT,0)+ISNULL(p.AMT,0) as GM, CASE ISNULL(s.AMT,0) WHEN 0 THEN 0 ELSE (ISNULL(s.AMT,0)+ISNULL(p.AMT,0))/s.AMT END GMPCT FROM 
	    ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, CASE MNACSGMT WHEN &apos;4500&apos; THEN 0 ELSE SUM(PERDBLNC) END AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~  ) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4300&apos; AND &apos;4500&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) s FULL OUTER JOIN 
        ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~  ) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4500&apos; AND &apos;5599&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) p ON s.JSTR=p.JSTR
       ) cy ON cm.JSTR=cy.JSTR 
	   FULL OUTER JOIN 
      ( SELECT s.JSTR, COALESCE(s.MNACSGMT,p.MNACSGMT) MNACSGMT, COALESCE(s.ACTDESCR,p.ACTDESCR) ACTDESCR, ISNULL(s.AMT,0) AMT, ISNULL(s.AMT,0)+ISNULL(p.AMT,0) as GM, CASE ISNULL(s.AMT,0) WHEN 0 THEN 0 ELSE (ISNULL(s.AMT,0)+ISNULL(p.AMT,0))/s.AMT END GMPCT FROM 
	    ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, CASE MNACSGMT WHEN &apos;4500&apos; THEN 0 ELSE SUM(PERDBLNC) END AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~-1) AND PERIODID=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4300&apos; AND &apos;4500&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) s FULL OUTER JOIN 
        ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~-1) AND PERIODID=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4500&apos; AND &apos;5599&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) p ON s.JSTR=p.JSTR
       ) pm ON cm.JSTR=pm.JSTR 
	   FULL OUTER JOIN 
      ( SELECT s.JSTR, COALESCE(s.MNACSGMT,p.MNACSGMT) MNACSGMT, COALESCE(s.ACTDESCR,p.ACTDESCR) ACTDESCR, ISNULL(s.AMT,0) AMT, ISNULL(s.AMT,0)+ISNULL(p.AMT,0) as GM, CASE ISNULL(s.AMT,0) WHEN 0 THEN 0 ELSE (ISNULL(s.AMT,0)+ISNULL(p.AMT,0))/s.AMT END GMPCT FROM 
	    ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, CASE MNACSGMT WHEN &apos;4500&apos; THEN 0 ELSE SUM(PERDBLNC) END AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~-1) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4300&apos; AND &apos;4500&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) s FULL OUTER JOIN 
        ( SELECT MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2) AS JSTR, LTRIM(RTRIM(ACTDESCR)) ACTDESCR, SUM(PERDBLNC) AMT FROM MKCGP..slbAccountSummary WHERE YEAR1=(@Request.paramFiscYear~-1) AND PERIODID&lt;=@Request.paramFiscPeriod~ AND ACTNUMBR_2 IN(&apos;@Request.paramLocGLStr~&apos;) AND MNACSGMT BETWEEN &apos;4500&apos; AND &apos;5599&apos; GROUP BY MNACSGMT, SUBSTRING(ACTNUMBR_4,2,3)+SUBSTRING(ACTNUMBR_5,1,2), LTRIM(RTRIM(ACTDESCR)) ) p ON s.JSTR=p.JSTR
       ) py ON cm.JSTR=py.JSTR 
GROUP BY COALESCE(cm.MNACSGMT,cy.MNACSGMT,pm.MNACSGMT,py.MNACSGMT), COALESCE(cm.ACTDESCR, cy.ACTDESCR, pm.ACTDESCR, py.ACTDESCR) 
ORDER BY MNACSGMT
"
																							Type="SQL"
																							>
